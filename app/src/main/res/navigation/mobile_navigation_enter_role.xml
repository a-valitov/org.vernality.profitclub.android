<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation_2"
    app:startDestination="@id/roleFragment">

    <fragment
        android:id="@+id/roleFragment"
        android:name="org.vernality.profitclub.view.fragments.RoleFragment"
        android:label="fragment_role"
        tools:layout="@layout/fragment_role" >

        <action
            android:id="@+id/action_roleFragment_to_enterRoleFragment"
            app:destination="@id/enterRoleFragment" />
    </fragment>

    <fragment
        android:id="@+id/enterRoleFragment"
        android:name="org.vernality.profitclub.view.fragments.EnterRoleDataFragment"
        android:label="fragment_enter_role"
        tools:layout="@layout/fragment_enter_role" >

        <action
            android:id="@+id/action_enterRoleFragment_to_BlankFragment"
            app:destination="@id/BlankFragment" />
    </fragment>

    <fragment
        android:id="@+id/BlankFragment"
        android:name="org.vernality.profitclub.view.fragments.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" >

    </fragment>


</navigation>